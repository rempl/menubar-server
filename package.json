{
  "name": "rempl-server-app",
  "version": "1.0.0",
  "description": "rempl basisjs basisjs-tools devtools",
  "dependencies": {
    "basisjs": "basisjs/basisjs#4b2406fedcb13c18590157ae8c53ddfc030e8ac5",
    "basisjs-tools-server": "basisjs/basisjs-tools-server#62c32721c57b7b16fa45e019c3939a29258290dd"
  },
  "devDependencies": {
    "electron": "^1.4.10",
    "electron-packager": "^8.3.0"
  },
  "main": "main.js",
  "scripts": {
    "build-mac": "rm -rf Rempl-darwin-x64; electron-packager . Rempl --platform=darwin --arch=x64 --version=1.4.10 --icon=Icon.icns --overwrite",
    "build-win": "rm -rf Rempl-win-x64; electron-packager . Rempl --platform=win32 --arch=x64 --version=1.4.10 --icon=Icon.icns --overwrite",
    "build": "build-mac",
    "start": "electron ."
  }
}
